                    "match": "\\b($applname|$applproperties|$appltitle|$batch|$detachedinstances|$display|$gui|$ioprint|$keyboard|$language|$logical|$nmforms|$password|$proc_tracing|$proc_tracing_addition|$putmess|$result|$setting|$text|$textexist|$user|$variation|$componentinfo|$componentname|$componenttype|$entname|$instancechildren|$instancedb|$instancedbmod|$instancehandle|$instancemod|$instancename|$instanceparent|$instances|$instancevalidation|$modelname|$bold|$clock|$condition|$date|$datim|$decode|$encode|$expression|$italic|$oprsys|$string|$stripattributes|$syntax|$typed|$ude|$ude|compile|$ude|copy|$ude|delete|$ude|exist|$ude|export|$ude|import|$ude|import|symboltable|$underline|$uuid|$concat|$displaylength|$length|$lowercase|$ltrim|$newstruct|$nlslocalelist|$nlstimezonelist|$number|$replace|$rscan|$rtrim|$scan|$split|$uppercase|$def_charset|$collhandle|$CurEntProperties|$curhits|$curkey|$curocc|$curoccvideo|$dbocc|$entinfo|$entityproperties|$foreign|$hits|$keycheck|$keyfields|$keymod|$keytype|$keyvalidation|$occcheck|$occcrc|$occdbmod|$occdel|$occhandle|$occmod|$occproperties|$occstatus|$occvalidation|$paintedocc|$relation|$rettype|$selectlist|$storetype|$subsetreturn|$totdbocc|$totkeys|$totocc|$dataerrorcontext|$dberror|$dberrortext|$error|$proc_profiling|$processinfo|$procerrorcontext|$procerror|$procReturnContext|$columnsyntax|$curline|$curword|$fieldcheck|$fielddbmod|$fielddbvalue|$fieldendmod|$fieldindb|$fieldinfo|$fieldmod|$fieldname|$fieldprofile|$fieldproperties|$fieldsyntax|$fieldvalidation|$fieldvalrep|$fieldvideo|$format|$next|$paintedfieldproperties|$previous|$properties|$valrep|$dirlist|$fileexists|$fileproperties|$ldir|$ldirlist|$lfileexists|$lfileproperties|$ude|load|$cellinfo|$char|$direction|$editmode|$formdb|$formdbmod|$formfocus|$formmod|$formmodality|$formname|$formtitle|$interactive|$labelproperties|$printing|$prompt|$runmode|$selblk|$windowproperties|$fieldhandle|$instancepath|$msgdata|$msgdst|$msgid|$msginfo|$msgsrc|$ocxhandle|$sys_ch